                                    </div>
                                </div>


                            </div>

                            <div class="row clearfix">

                                <div class="col-sm-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            <input type="text" name="streetName" id="streetName" class="form-control" required maxlength="255">
                                            <label class="form-label">@L("streetName")</label>

                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group form-float">
